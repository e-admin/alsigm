			<include refid="columns" />
		FROM
			<include refid="table" />
		WHERE
			id_asiento_registral=#value:INTEGER#
		ORDER BY ds_identificador
	</select>

	<select id="getInteresadoVO" parameterClass="java.lang.String" resultMap="interesado">
		SELECT
