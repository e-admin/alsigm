	<properties>
		<unpack.config.directory>${project.build.directory}/generated-resources</unpack.config.directory>
	</properties>

	<build>
		<finalName>SIGEM_RegistroTelematicoWS</finalName>

		<filters>
			<filter>${filter.generic.system}</filter>
		</filters>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>

			<resource>
				<directory>${unpack.config.directory}</directory>
			</resource>
		</resources>

		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack-dependencies</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<includeClassifiers>configurableSigem</includeClassifiers>
							<includeTypes>zip</includeTypes>
							<excludeTypes>jar</excludeTypes>
							<type>zip</type>
							<outputDirectory>
								${unpack.config.directory}
							</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>


	<profiles>

		<!-- Perfiles de construccion en funcion del S.O. -->
		<profile>
			<id>unix</id>
			<activation>
				<property>
					<name>unix</name>
				</property>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<filter.generic.system>${project.basedir}/../../SIGEM_Config/src/main/filters/system/unix.properties</filter.generic.system>
			</properties>
		</profile>
		<profile>
			<id>win</id>
			<activation>
				<property>
					<name>win</name>
				</property>
			</activation>
			<properties>
				<filter.generic.system>${project.basedir}/../../SIGEM_Config/src/main/filters/system/windows.properties</filter.generic.system>
			</properties>
		</profile>
		<profile>
			<id>generate-wsdl</id>
			<activation>

      		<property>
      			<name>generateWSDL</name>
      			<value>true</value>
      		</property>

      		</activation>

			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>axistools-maven-plugin</artifactId>
						<version>1.3</version>

						<configuration>
							<!-- className>ieci.tecdoc.sgm.catalogo.ws.server.CalendarioWebService</className -->
							<!-- outputFileName>${basedir}/src/main/webapp/wsdl2/CalendarioWebService.wsdl</outputFileName -->
							<!-- targetNamespace>http://server.ws.calendario.sgm.tecdoc.ieci</targetNamespace -->
							<!-- serviceName>CalendarioWebService</serviceName -->

							<filename>${project.artifactId}.wsdl</filename>
							<namespace>http://server.ws.registro.sgm.tecdoc.ieci</namespace>
							<namespaceImpl>http://server.ws.registro.sgm.tecdoc.ieci</namespaceImpl>
							<classOfPortType>ieci.tecdoc.sgm.registro.ws.server.RegistroTelematicoWebService</classOfPortType>
							<location>http://localhost:8080/SIGEM_RegistroTelematicoWS/services/RegistroTelematicoWebService</location>
							<bindingName>RegistroTelematicoWebServiceSoapBinding</bindingName>
							<style>WRAPPED</style>
							<use>literal</use>
						</configuration>
						<executions>
							<execution>
								<goals>
									<goal>java2wsdl</goal>
								</goals>
							</execution>
						</executions>
						<dependencies>
							<dependency>
								<groupId>axis</groupId>
								<artifactId>axis</artifactId>
								<version>1.3</version>
							</dependency>

						</dependencies>
					</plugin>
				</plugins>
			</build>

		</profile>

		<profile>
			<id>generateCSVConnectorWSDL</id>
			<activation>

      		<property>
      			<name>generateCSVConnectorWSDL</name>
      			<value>true</value>
      		</property>

      		</activation>

			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>axistools-maven-plugin</artifactId>
						<version>1.3</version>

						<configuration>
							<!-- className>ieci.tecdoc.sgm.catalogo.ws.server.CalendarioWebService</className -->
							<!-- outputFileName>${basedir}/src/main/webapp/wsdl2/CalendarioWebService.wsdl</outputFileName -->
							<!-- targetNamespace>http://server.ws.calendario.sgm.tecdoc.ieci</targetNamespace -->
							<!-- serviceName>CalendarioWebService</serviceName -->

							<filename>${project.artifactId}-csv-connector.wsdl</filename>
							<namespace>http://server.connector.csv.ws.sgm.tecdoc.ieci</namespace>
							<namespaceImpl>http://server.connector.csv.ws.sgm.tecdoc.ieci</namespaceImpl>
							<classOfPortType>ieci.tecdoc.sgm.ws.csv.connector.server.AplicacionExternaCSVConnectorWS</classOfPortType>
							<location>http://localhost:8080/SIGEM_RegistroTelematicoWS/services/AplicacionExternaCSVConnectorService</location>
							<bindingName>AplicacionExternaCSVConnectorServiceSoapBinding</bindingName>
							<style>WRAPPED</style>
							<use>literal</use>
						</configuration>
						<executions>
							<execution>
								<goals>
									<goal>java2wsdl</goal>
								</goals>
							</execution>
						</executions>
						<dependencies>
							<dependency>
								<groupId>axis</groupId>
