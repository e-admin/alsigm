	<build>
		<filters>
			<filter>${filter.generic.system}</filter>
		</filters>
		<resources>
				<resource>
					<directory>src/main/resources</directory>
					<filtering>true</filtering>
				</resource>

				<resource>
					<directory>${unpack.config.directory}</directory>
				</resource>
		</resources>

		<finalName>SIGEM_RegistroTelematicoWeb</finalName>

		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack-dependencies</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<includeClassifiers>configurableSigem</includeClassifiers>
							<includeTypes>zip</includeTypes>
							<excludeTypes>jar</excludeTypes>
							<type>zip</type>
							<outputDirectory>
								${unpack.config.directory}
							</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

