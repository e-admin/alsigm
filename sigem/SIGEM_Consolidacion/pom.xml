<?xml version="1.0" encoding="UTF-8" standalone="no"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<artifactId>sigem_main</artifactId>
		<groupId>es.ieci.tecdoc.sigem</groupId>
		<version>3.0.1-M3</version>
	</parent>

	<artifactId>sigem_consolidacion</artifactId>
	<name>SIGEM_Consolidacion</name>

	<dependencies>

		<!-- SIGEM dependencies -->

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_core</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>com.thoughtworks.xstream</groupId>
					<artifactId>xstream</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_entidades</artifactId>
			<version>${pom.version}</version>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_consolidacionConfigLoader</artifactId>
			<version>${pom.version}</version>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_entidadesWsClient</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>wsdl4j</groupId>
					<artifactId>wsdl4j</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_registroTelematico</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>es.ieci.tecdoc.sigem</groupId>
					<artifactId>sigem_autenticacion</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
		  <groupId>es.ieci.tecdoc.sigem</groupId>
		  <artifactId>sigem_registroTelematicoWsClient</artifactId>
		  <version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>es.ieci.tecdoc.sigem</groupId>
					<artifactId>sigem_autenticacion</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_registroPresencial</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>commons-lang</groupId>
					<artifactId>commons-lang</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
		  <groupId>es.ieci.tecdoc.sigem</groupId>
		  <artifactId>sigem_registroPresencialWsClient</artifactId>
		  <version>${pom.version}</version>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>SIGEM_MensajesCortos</artifactId>
			<version>${pom.version}</version>
		</dependency>


		<!-- Third party dependencies -->

		<dependency>
		  <groupId>commons-lang</groupId>
		  <artifactId>commons-lang</artifactId>
		  <version>${commons.lang.version}</version>
		</dependency>

		<dependency>
			<groupId>com.sun.media</groupId>
			<artifactId>jai-codec</artifactId>
			<version>1.1.3</version>
		</dependency>

	</dependencies>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<version>${assembly.plugin.version}</version>
				<configuration>
					<descriptors>
						<descriptor>
							${basedir}/src/main/assembly/dep.xml
						</descriptor>
					</descriptors>
				</configuration>
				<executions>
					<execution>
						<id>assembly-configurable-resources</id>
						<goals>
							<goal>single</goal>
						</goals>
						<phase>package</phase>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
