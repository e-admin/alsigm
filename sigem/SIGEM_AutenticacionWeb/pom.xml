			<scope>provided</scope>
		</dependency>


	</dependencies>

	<build>
		<finalName>SIGEM_AutenticacionWeb</finalName>
		<filters>
			<filter>${filter.generic.system}</filter>
		</filters>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
	</build>

	<profiles>
		<!-- Perfiles de construccion en funcion del S.O. -->
		<profile>
			<id>unix</id>
			<activation>
				<property>
					<name>unix</name>
				</property>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<filter.generic.system>${project.basedir}/../SIGEM_Config/src/main/filters/system/unix.properties</filter.generic.system>
			</properties>
		</profile>
		<profile>
			<id>win</id>
			<activation>
				<property>
					<name>win</name>
				</property>
			</activation>
			<properties>
				<filter.generic.system>${project.basedir}/../SIGEM_Config/src/main/filters/system/windows.properties</filter.generic.system>
			</properties>
		</profile>

		<profile>
			<id>websphere</id>
			<activation>
				<property>
					<name>websphere</name>
				</property>
			</activation>
			<build>
				<finalName>SIGEM_AutWeb</finalName>
			</build>
		</profile>
	</profiles>
</project>