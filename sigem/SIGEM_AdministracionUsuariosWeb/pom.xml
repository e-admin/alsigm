<?xml version="1.0" encoding="UTF-8" standalone="no"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>es.ieci.tecdoc.sigem</groupId>
		<artifactId>sigem_main</artifactId>
		<version>3.0.1-M3</version>
	</parent>
	
	<artifactId>sigem_administracionUsuariosWeb</artifactId>
	<packaging>war</packaging>
	
	
	
	<dependencies>
		
				
		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_core</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>com.thoughtworks.xstream</groupId>
					<artifactId>xstream</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_clasesBase</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>commons-net</groupId>
					<artifactId>commons-net</artifactId>
				</exclusion>
				<exclusion>
					<groupId>xerces</groupId>
					<artifactId>xercesImpl</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>xerces</groupId>
			<artifactId>xercesImpl</artifactId>
			<version>2.6.2</version>
		</dependency>
		
		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_autenticacionUsuarios</artifactId>
			<version>${pom.version}</version>
		</dependency>
		
		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_autenticacionUsuariosWsClient</artifactId>
			<version>${pom.version}</version>
		</dependency>
		
		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_utilidadesAdministracion</artifactId>
			<version>${pom.version}</version>
			<exclusions>
				<exclusion>
					<groupId>es.ieci.tecdoc.sigem</groupId>
					<artifactId>sigem_administracionSesionesBackOfficeWsClient</artifactId>
				</exclusion>
				<exclusion>
					<groupId>es.ieci.tecdoc.sigem</groupId>
					<artifactId>sigem_entidades</artifactId>
				</exclusion>
				<exclusion>
					<groupId>es.ieci.tecdoc.sigem</groupId>
					<artifactId>sigem_estructuraOrganizativa</artifactId>
				</exclusion>
				<exclusion>
					<groupId>es.ieci.tecdoc.sigem</groupId>
					<artifactId>sigem_estructuraOrganizativaCore</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>es.ieci.tecdoc.sigem</groupId>
			<artifactId>sigem_administracionSesionesAdmWsClient</artifactId>
			<version>${pom.version}</version>
		</dependency>
		
		
		
		<dependency>
			<groupId>struts</groupId>
			<artifactId>struts</artifactId>
			<version>1.1</version>
			<exclusions>
				<exclusion>
					<groupId>javax.sql</groupId>
					<artifactId>jdbc-stdext</artifactId>
				</exclusion>
				<exclusion>
					<groupId>struts</groupId>
					<artifactId>struts-legacy</artifactId>
				</exclusion>  			
  			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>servlet-api</artifactId>
			<version>2.3</version>
			<scope>provided</scope>
		</dependency>
		
		<dependency>
			<groupId>axis</groupId>
			<artifactId>axis</artifactId>
			<version>1.3</version>
		</dependency>
		
		<dependency>
			<groupId>commons-validator</groupId>
			<artifactId>commons-validator</artifactId>
			<version>1.0.2</version>
			<exclusions>
				<exclusion>
					<groupId>junit</groupId>
					<artifactId>junit</artifactId>
				</exclusion>
				<exclusion>
					<groupId>xml-apis</groupId>
					<artifactId>xml-apis</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
			<version>1.7.0</version>
		</dependency>
		
		<dependency>
			<groupId>commons-digester</groupId>
			<artifactId>commons-digester</artifactId>
			<version>1.8</version>
		</dependency>
		
		<dependency>
			<groupId>displaytag</groupId>
			<artifactId>displaytag</artifactId>
			<version>1.1.1</version>
			<exclusions>
				<exclusion>
					<groupId>com.lowagie</groupId>
					<artifactId>itext</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.slf4j</groupId>
					<artifactId>jcl104-over-slf4j</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.slf4j</groupId>
					<artifactId>slf4j-log4j12</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		

	</dependencies>
	
	<build>
		<finalName>SIGEM_AdministracionUsuariosWeb</finalName>
		<filters>
			<filter>${filter.generic.system}</filter>
		</filters>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>		
	</build>
	
	<profiles>
		<!-- Perfiles de construccion en funcion del S.O. -->
		<profile>
			<id>unix</id>
			<activation>
				<property>
					<name>unix</name>
				</property>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<filter.generic.system>${project.basedir}/../SIGEM_Config/src/main/filters/system/unix.properties</filter.generic.system>
			</properties>
		</profile>
		<profile>
			<id>win</id>
			<activation>
				<property>
					<name>win</name>
				</property>
			</activation>
			<properties>
				<filter.generic.system>${project.basedir}/../SIGEM_Config/src/main/filters/system/windows.properties</filter.generic.system>
			</properties>
		</profile>
		
		<profile>
			<id>websphere</id>
			<activation>
				<property>
					<name>websphere</name>
				</property>
			</activation>
			<build>
				<finalName>SIGEM_AdmUsrWeb</finalName>
			</build>
		</profile>
	</profiles>	
	
	
</project>