<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<artifactId>InvesSicres</artifactId>
		<groupId>es.ieci.tecdoc.isicres</groupId>
		<version>8.0.5-M1</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>ISicres-Audit</artifactId>
	<name>ISicres-Audit</name>
	<packaging>jar</packaging>
	<description>Modulo de logica del Auditoria de InvesSicres</description>


	<dependencies>

		<dependency>
			<groupId>es.ieci.tecdoc.fwktd.modules.fwktd-audit</groupId>
			<artifactId>fwktd-audit-api</artifactId>
		<version>${fwktd.audit.version}</version>
		</dependency>

		<dependency>
			<groupId>es.ieci.tecdoc.fwktd.modules.fwktd-audit</groupId>
			<artifactId>fwktd-audit-integration</artifactId>
		<version>${fwktd.audit.version}</version>
		</dependency>

		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>${log4j.version}</version>
		</dependency>

		<dependency>
			<groupId>dom4j</groupId>
			<artifactId>dom4j</artifactId>
			<version>${dom4j.version}</version>
		</dependency>

		<dependency>
			<groupId>trove</groupId>
			<artifactId>trove</artifactId>
			<version>1.0.2</version>
		</dependency>

		<!-- Test -->
		<dependency>
			<groupId>es.ieci.tecdoc.fwktd</groupId>
			<artifactId>fwktd-test</artifactId>
			<version>${fwktd.version}</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>${junit.version}</version>
			<scope>test</scope>
		</dependency>

	</dependencies>

	<build>

		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
					<encoding>iso-8859-1</encoding>
					<fork>true</fork>
          <!--					<executable>${JAVA_1_5_HOME}/bin/javac</executable>-->
				</configuration>
			</plugin>

		</plugins>
	</build>


</project>