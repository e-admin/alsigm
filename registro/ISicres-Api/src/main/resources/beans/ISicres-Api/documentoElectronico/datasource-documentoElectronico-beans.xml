<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	

	<!-- JNDI Datasource -->
	<bean id="isicresDocumentoElectronicoDataSource" class="es.ieci.tecdoc.fwktd.core.spring.configuration.jdbc.datasource.MultiEntityJndiDatasource">
		<property name="jndiBaseName">
			<value>${isicres.database.jndiName}</value>
		</property>
	</bean>


</beans>