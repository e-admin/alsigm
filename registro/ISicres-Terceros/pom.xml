<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>InvesSicres</artifactId>
		<groupId>es.ieci.tecdoc.isicres</groupId>
		<version>8.0.5-M1</version>
	</parent>
	<artifactId>ISicres-Terceros</artifactId>
	<name>ISicres-Terceros</name>
	<dependencies>
		
		<dependency>
			<groupId>es.ieci.tecdoc.fwktd</groupId>
			<artifactId>fwktd-server</artifactId>
			<version>${fwktd.version}</version>
		</dependency>
		<dependency>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
			<version>1.8.3</version>
		</dependency>
		<dependency>
			<groupId>javax.validation</groupId>
			<artifactId>validation-api</artifactId>
			<version>1.0.0.GA</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.12</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>postgresql</groupId>
			<artifactId>postgresql</artifactId>
			<version>8.3-603.jdbc3</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>es.ieci.tecdoc.fwktd</groupId>
			<artifactId>fwktd-test</artifactId>
			<version>${fwktd.version}</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>1.1.1</version>
			<scope>test</scope>
		</dependency>
	</dependencies>

	<build>

	</build>
	<profiles>
		<profile>
			<activation>
				<activeByDefault>false</activeByDefault>
				<property>
					<name>exportdb</name>
				</property>
			</activation>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>dbunit-maven-plugin</artifactId>
						<version>1.0-beta-3</version>

						<!--jar file that has the jdbc driver -->
						<dependencies>
							<dependency>
								<groupId>postgresql</groupId>
								<artifactId>postgresql</artifactId>
								<version>8.3-603.jdbc3</version>
							</dependency>
						</dependencies>

						<!-- common configurations -->
						<configuration>
							<driver>org.postgresql.Driver</driver>
							<url>jdbc:postgresql://10.228.75.106:5432/ISicres_3_0</url>
							<username>postgres</username>
							<password>postgres</password>
						</configuration>

						<executions>
							<execution>
								<phase>generate-resources</phase>
								<goals>
									<goal>export</goal>
								</goals>
								<!-- specific configurations -->
								<configuration>
									<format>flat</format>
									<tables>
										<table>
											<name>scr_regint</name>
										</table>
										<table>
											<name>scr_pjur</name>
										</table>
										<table>
											<name>scr_typedoc</name>
										</table>
										<table>
											<name>scr_pfis</name>
										</table>

										<table>
											<name>scr_address</name>
										</table>
										<table>
											<name>scr_dom</name>
										</table>
										<table>
											<name>scr_typeaddress</name>
										</table>
										<table>
											<name>scr_addrtel</name>
										</table>
										<!-- <table> -->
										<!-- <name>scr_country</name> -->
										<!-- </table> -->
										<table>
											<name>scr_prov</name>
										</table>
										<table>
											<name>scr_cities</name>
										</table>
										<table>
											<name>scr_country</name>
										</table>
										<table>
											<name>scr_contador</name>
										</table>
									</tables>
									<dest>target/dbunit/export.xml</dest>
								</configuration>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>

</project>