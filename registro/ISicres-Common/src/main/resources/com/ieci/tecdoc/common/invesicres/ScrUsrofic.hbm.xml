<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.ieci.tecdoc.common.invesicres.ScrUsrofic" 
    table="SCR_USROFIC"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="SCR_USROFIC"
    </meta>

    <composite-id>
        <meta attribute="class-description" inherit="false">
           @hibernate.id
            generator-class="assigned"
        </meta>
        <key-property 
            name="id" 
            column="ID" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="ID"
                length="10"
            </meta> 
        </key-property>
        <key-property 
            name="iduser" 
            column="IDUSER" 
            type="int"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="IDUSER"
                length="10"
            </meta> 
        </key-property>
    </composite-id>    


    <!-- associations -->
    <!-- bi-directional many-to-one association to ScrOfic -->
    <many-to-one
        name="scrOfic"
        class="com.ieci.tecdoc.common.invesicres.ScrOfic"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="IDOFIC"         
        </meta>
        <column name="IDOFIC" />
    </many-to-one>

</class>
</hibernate-mapping>
