<?xml version="1.0" encoding="UTF-8"?>
<ISicres-Extension-Files-Configuration>
	<!-- Operativa por defecto a realizar para la extensiones
		 TRUE - Se fuerza cuadro de dialogo de abrir/guardar ficheros
		 FALSE - Se visualiza el fichero en el navegador
	-->
	<showDialogDownloadFileByDefault>${showdialog.extension.files.default}</showDialogDownloadFileByDefault>

	<extensions-configuration>

		<!-- Ficheros visualizados por ActiveX (ides.cab) -->
		<extension-configuration>
			<extension>JPG</extension>
			<showDialogDownloadFile>false</showDialogDownloadFile>
		</extension-configuration>

		<extension-configuration>
			<extension>JPEG</extension>
			<showDialogDownloadFile>false</showDialogDownloadFile>
		</extension-configuration>

		<extension-configuration>
			<extension>TIF</extension>
			<showDialogDownloadFile>false</showDialogDownloadFile>
		</extension-configuration>

		<extension-configuration>
			<extension>TIFF</extension>
			<showDialogDownloadFile>false</showDialogDownloadFile>
		</extension-configuration>

		<extension-configuration>
			<extension>BMP</extension>
			<showDialogDownloadFile>false</showDialogDownloadFile>
		</extension-configuration>

		<!-- Coleccion con las diferentes extensiones y operativa a seguir -->
		<extension-configuration>

			<!-- Se define la extension del fichero -->
			<extension>XADES</extension>
			<!--
				Operativa a realizar para la extensiones
				 TRUE - Se fuerza cuadro de dialogo de abrir/guardar ficheros
				 FALSE - Se visualiza el fichero en el navegador
			-->
			<showDialogDownloadFile>true</showDialogDownloadFile>
		</extension-configuration>

		<extension-configuration>
			<!-- Se define la extension del fichero -->
			<extension>XSIG</extension>
			<!--
				Operativa a realizar para la extensiones
				 TRUE - Se fuerza cuadro de dialogo de abrir/guardar ficheros
				 FALSE - Se visualiza el fichero en el navegador
			-->
			<showDialogDownloadFile>true</showDialogDownloadFile>
		</extension-configuration>

		<!-- Ejemplo de configuracion de mas extensiones
		<extension-configuration>
			<extension>txt</extension>
			<showDialogDownloadFile>true</showDialogDownloadFile>
		</extension-configuration>

		<extension-configuration>
			<extension>XML</extension>
			<showDialogDownloadFile>false</showDialogDownloadFile>
		</extension-configuration>

		-->

	</extensions-configuration>

</ISicres-Extension-Files-Configuration>