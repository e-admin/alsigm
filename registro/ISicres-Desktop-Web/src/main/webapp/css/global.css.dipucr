/* Para fijar la cabecera de la tabla de resultados de consulta*/
div.fixedHeaderTable {
    position: relative;
    z-index:1;
}

div.fixedHeaderTable tbody {
    height: 400px;
    overflow-y: auto;
    overflow-x: auto;
    background-color: #FFFFFF;
}
div.fixedHeaderTable thead td, div.fixedHeaderTable thead th {
    position:relative;
}

   /* IE7 hacks */
div.fixedHeaderTable {
    *position: relative;
    *height: 400px;
    *overflow-y: scroll;
   	*overflow-x: scroll;
}

div.fixedHeaderTable thead tr {
    *position: relative;
    _position: absolute;
    *top: expression(this.offsetParent.scrollTop-2);
}

div.fixedHeaderTable tbody {
    *height: auto;
    *position:absolute;
	*top:50px;
	*background-color: #FFFFFF;
}

	/* IE6 hacks */
div.fixedHeaderTable {
	_width:expression(this.offsetParent.clientWidth-20);
	_overflow: auto;
	_overflow-y: scroll;
	_overflow-x: scroll;
}
div.fixedHeaderTable thead tr {
  _position: relative
}

