<CAMPO_FILA
	etiqueta="Documento_Electronico">
	<CAMPO_TEXTO
		etiqueta="IE_Origen" />
	<CAMPO_TEXTO
		etiqueta="IE_Estado_Elaboracion" />
	<CAMPO_TEXTO
		etiqueta="IE_Nombre_Formato" />
	<CAMPO_TEXTO
		etiqueta="IE_Tipo_Documental" />
	<CAMPO_TEXTO
		etiqueta="IE_Tipo_Firma" />
	<CAMPO_TEXTO
		etiqueta="IE_Valor_CSV" />
	<CAMPO_TEXTO
		etiqueta="IE_Definicion_Generacion_CSV" />
	<CAMPO_TEXTO
		etiqueta="IE_Identificador_Documento_Origen" />
	<!-- Campo tipo Documento Electrónico -->
	<CAMPO_DOCUMENTO_ELECTRONICO
		etiqueta="IE_Id_Interno">
		<!-- Datos de Ubicación -->
		<UBICACION
			tipo="BINARIO">
			<BINARIO>
				<CONTENIDO>
				</CONTENIDO>
			</BINARIO>

			<REPOSITORIO
				tipo="ALFRESCO"
				idRepositorio="idRepAlfresco"
				localizador="localizador" />

		</UBICACION>
		<!-- Almacenamiento del documento -->
		<ALMACENAMIENTO
			tipo="REPOSITORIO"
			nombre="prueba1"
			extension="pdf">
			<!-- Sólo se especifica el tipo se el WS tiene que almacenar el fichero -->
			<!-- Obligatorios -->
			<!-- Obligatorio para tipo almacenamiento ECM -->

			<REPOSITORIO
				tipo="INVESDOC"
				idRepositorio="IdRepositorioInvesdoc"
				localizador="LocalizadorInvesdoc" />

			<!-- Opcionales -->
			<DESCRIPCION>Fichero de pruebas</DESCRIPCION>
			<!-- Opcional -->
			<CLASIFICADOR>pruebas</CLASIFICADOR>
			<!-- Opcional -->
		</ALMACENAMIENTO>
	</CAMPO_DOCUMENTO_ELECTRONICO>

	<!-- Campo para almacenar la firma -->
	<CAMPO_DOCUMENTO_ELECTRONICO
		etiqueta="IE_Firma">
		<!-- Datos de Ubicación -->
		<UBICACION
			tipo="BINARIO">
			<BINARIO>
				<CONTENIDO>
				</CONTENIDO>
			</BINARIO>

			<REPOSITORIO
				tipo="ALFRESCO"
				idRepositorio="idRepAlfresco"
				localizador="localizador" />

		</UBICACION>
		<!-- Almacenamiento del documento -->
		<ALMACENAMIENTO
			tipo="REPOSITORIO"
			nombre="prueba1"
			extension="pdf">
			<!-- Sólo se especifica el tipo se el WS tiene que almacenar el fichero -->
			<!-- Obligatorios -->
			<!-- Obligatorio para tipo almacenamiento ECM -->

			<REPOSITORIO
				tipo="INVESDOC"
				idRepositorio="IdRepositorioInvesdoc"
				localizador="LocalizadorInvesdoc" />

			<!-- Opcionales -->
			<DESCRIPCION>Firma del Documento</DESCRIPCION>
			<!-- Opcional -->
			<CLASIFICADOR>Firmas</CLASIFICADOR>
			<!-- Opcional -->
		</ALMACENAMIENTO>
	</CAMPO_DOCUMENTO_ELECTRONICO>

</CAMPO_FILA>